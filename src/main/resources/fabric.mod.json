{
	"schemaVersion": 1,
	"id": "enchant-outline",
	"version": "${version}",
	"name": "Enchantment Glint Outline",
	"description": "Adds a fancy outline to enchanted items. Works with resource packs and vanilla shaders. Sodium compatible",
	"authors": [
		"Da0neDatGotAway"
	],
	"contributors": [
		"yo314159at",
		"Igrium"
	],
	"license": "CC0-1.0",
	"icon": "assets/enchantment-glint-outline/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.enchantoutline.EnchantmentGlintOutline"
		],
		"modmenu": [
			"net.enchantoutline.config.ModMenuIntegration"
		]
	},
	"accessWidener" : "enchantment-glint-outline.accesswidener",
	"mixins": [
		"enchantment-glint-outline.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.1",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"modmenu": "*",
		"yacl": "*"
	}
}